;--------------------------------------------------------------------------
;                Datenverbindung 
;--------------------------------------------------------------------------

[CONNECTION]

; -----------------------------------------------------
; An ZMS angeschlossen ?
zmslink = Ja

; -----------------------------------------------------
; Einstellung der Verbindungsart
; 0 = X.25
; 1 = TCP/IP
; 2 = TCP/IP über Einwahl (DFÜ)
; 3 = FTP über Einwahl
Linktype = 0


; -----------------------------------------------------
; Routereinstellungen für X25-Verbindung
; 0 = Einzelanlage mit einer Meldestelle
; 1 = Einzelanlage mit zwei Meldestellen, Zugmeldungen erfolgen unabhängig von der Fahrtrichtung an beide Meldestellen
; 2 = Doppelanlage mit zwei Meldestellen, Zugmeldungen erfolgen fahrtrichtingsabhängig an jeweils zuständige Meldestelle
;     ( Regelfahrt wird über X25port1 gemeldet, Gleiswechselbetrieb über zweite SCT )                                     
; 3 = Einzelanlage mit zwei Meldestellen, Zugmeldungen erfolgen fahrtrichtingsabhängig an jeweils zuständige Meldestelle
;     ( Regelfahrt wird über X25port1 gemeldet, Gleiswechselbetrieb über X25port2 )                                     
; 4 = Doppelanlage mit einer Meldestelle, zweite SCT sendet über X25-Verbindung der ersten SCT
;     (Konfiguration für die SCT mit Verbindung zur Meldestelle)
; 5 = Doppelanlage mit einer Meldestelle, zweite SCT sendet über X25-Verbindung der ersten SCT
;     (Konfiguration für die SCT ohne direkte Verbindung zur Meldestelle)
RouterSetup = $$Router Setup@0$$

;-------------------------------------------------------------------------
; Routerparameter für X25- und SCT-Verbindung

; nur benutzt bei RouterSetup 0, 1, 2, 3 und 4
X25port1 = $$1. COM@1$$
X25varDTRcall1 = $$1. DTR toggeln@Ja$$

; nur benutzt bei RouterSetup 1 und 3
X25port2 = $$2. COM@6$$
X25varDTRcall2 = $$2. DTR toggeln@Ja$$

; nur benutzt bei RouterSetup 2, 4 und 5
SCTlocalport = 20085
SCThostname = 10.0.0.2
SCThostport = 20085

; Aktuelle Störungsmeldungen nach Verbindungsaufbau automatisch senden.
Mas90GA = Ja

;--------------------------------------------------------------------------
; ZBG Interface (Zugbildgenerator)
ZBG = Nein

;--------------------------------------------------------------------------
;RailWeight Interface
[RWDATA]
Timeout = 1

